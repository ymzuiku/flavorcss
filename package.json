{
  "name": "flavorcss",
  "version": "0.6.11",
  "description": "full build css",
  "keywords": [
    "ui",
    "css",
    "atmoic css"
  ],
  "main": "esm/index.js",
  "module": "es/index.js",
  "types": "lib/index.ts",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --watch",
    "build:esm": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=esm --bundle --format=esm --jsx-factory=aoife --jsx-fragment=aoife.Frag --splitting --minify",
    "build:cjs": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=cjs --bundle --format=cjs --jsx-factory=aoife --jsx-fragment=aoife.Frag --minify",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "fs-extra": "^9.0.0",
    "esbuild": "^0.13.1",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.2.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "bugs": {
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "homepage": "https://flavor.writeflowy.com"
}
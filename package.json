{
  "name": "flavorcss",
  "version": "0.8.2",
  "description": "full build css",
  "keywords": [
    "ui",
    "css",
    "atmoic css"
  ],
  "main": "cjs/cjs.js",
  "module": "esm/esm.js",
  "types": "lib/index.ts",
  "exports": {
    "import": "./esm/esm.js",
    "require": "./cjs/cjs.js"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --watch",
    "build:esm": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/esm.ts --outdir=esm --bundle --format=esm --jsx-factory=aoife --jsx-fragment=aoife.Frag --splitting --minify",
    "build:cjs": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/cjs.ts --outdir=cjs --bundle --format=cjs --jsx-factory=aoife --jsx-fragment=aoife.Frag --minify",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "esbuild": "^0.13.1",
    "esbuild-jest": "^0.5.0",
    "fs-extra": "^9.1.0",
    "jest": "^27.2.1",
    "prettier": "^2.5.1",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "printWidth": 120,
    "singleQuote": false,
    "jsxBracketSameLine": true,
    "useTabs": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "bugs": {
    "url": "git+https://github.com/ymzuiku/flavorcss.git"
  },
  "homepage": "https://flavor.writeflowy.com"
}